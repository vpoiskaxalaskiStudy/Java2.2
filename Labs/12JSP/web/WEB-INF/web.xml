<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns="http://xmlns.jcp.org/xml/ns/javaee"
         xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
         xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-app_4_0.xsd"
         version="4.0">
    <filter>
        <filter-name>encodingFilter</filter-name>
        <filter-class>EncodingFilter</filter-class>
    </filter>
    <filter-mapping>
        <filter-name>encodingFilter</filter-name>
        <url-pattern>/</url-pattern>
    </filter-mapping>
    <servlet>
        <servlet-name>PrintInfo</servlet-name>
        <servlet-class>PrintInfo</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>PrintInfo</servlet-name>
        <url-pattern>/printinfo</url-pattern>
    </servlet-mapping>
    
    <servlet>
        <servlet-name>login</servlet-name>
        <servlet-class>login</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>login</servlet-name>
        <url-pattern>/login</url-pattern>
    </servlet-mapping>
    <servlet>
        <servlet-name>InputName</servlet-name>
        <servlet-class>InputName</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>InputName</servlet-name>
        <url-pattern>/InputName</url-pattern>
    </servlet-mapping>
    <servlet>
        <servlet-name>ForwardBack</servlet-name>
        <servlet-class>ForwardBack</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>ForwardBack</servlet-name>
        <url-pattern>/ForwardBack</url-pattern>
    </servlet-mapping>
    <servlet>
        <servlet-name>Register</servlet-name>
        <servlet-class>Register</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>Register</servlet-name>
        <url-pattern>/register</url-pattern>
    </servlet-mapping>
    <servlet>
        <servlet-name>DataCard</servlet-name>
        <servlet-class>DataCard</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>DataCard</servlet-name>
        <url-pattern>/DataCard</url-pattern>
    </servlet-mapping>
    <servlet>
        <servlet-name>ForwardCard</servlet-name>
        <servlet-class>ForwardCard</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>ForwardCard</servlet-name>
        <url-pattern>/ForwardCard</url-pattern>
    </servlet-mapping>
</web-app>